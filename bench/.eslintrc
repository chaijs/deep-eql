{
  "extends": [ "strict/test" ],
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-eval": 0,
    "no-new-wrappers": 0,
    "no-array-constructor": 0,
    "no-new-object": 0,
    "no-empty-function": 0,
    "no-undefined": 0,
    "no-console": 0,
    "id-length": 0
  }
}
